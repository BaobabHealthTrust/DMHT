<div style="font-size: 22pt;">
  <ul class="list" style="height:270px;width:500px;">
    <% for encounter in @encounters do %>
      <li class="data <%= cycle('even', 'odd') %>">
        <%=h encounter.name.humanize %>
        <div class="summary"><%= encounter.to_s.gsub(/\n/, '<br/>') -%></div>
      </li>  
    <% end %>
  </ul>
</div>