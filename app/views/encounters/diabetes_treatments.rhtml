  <% default={
    :allowFreeText => 'true'
    } %>
  <%
    @select_years = ["", "Unknown"]
    this_year = Date.today.year
    begin @select_years << Array.new(1){this_year.to_s}; this_year = this_year - 1; end while (this_year != 1949)
  %>

