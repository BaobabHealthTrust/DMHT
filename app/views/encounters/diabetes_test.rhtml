<style type="text/css">
  #space { display: inline;}
  #Unknown { display: none;}
  
</style>

<%= @destination_page =  "/patients/mastercard/" + @patient.id.to_s %>
<script src="/javascripts/cascade.js" type="text/javascript" language="javascript"></script>
<script type="text/javascript" language="javascript">
  var tt_cancel_destination = "<%= @destination_page -%>"

  var my_date =[];

  function resetCreatinineRange(current_page) {
    var unit = $('creatinine_unit').value;

    if (unit == 'µmol/l') {
      $(current_page).setAttribute('min', 45);
      $(current_page).setAttribute('max', 330.0);
    }
  }

  /* convert μmol/l to mg/dl*/
  function updateCreatinineResult(){
    var unit = $('creatinine_unit').value;

      if (unit == 'µmol/l') {
        $('creatinine_result_value_numeric').value = parseFloat(tstInputTarget.value / 91).toFixed(1);
      }
      else{
        $('creatinine_result_value_numeric').value = tstInputTarget.value;
      }
  }
</script>


<form action="/encounters/create/diabetes_test" method="post">

  <% default={
    :allowFreeText => 'true',
  } %>

  <%= hidden_field_tag "encounter[encounter_type_name]", "DIABETES TEST" %>
  <%= hidden_field_tag "encounter[patient_id]", @patient.id %>
  <%= hidden_field_tag "encounter[encounter_datetime]", DateTime.now %>
  <%= hidden_field_tag "encounter[provider_id]", session[:user_id] %>

  <%
  @select_years = ["", "Unknown"]
  this_year = Date.today.year
  begin @select_years << Array.new(1){this_year}; this_year = this_year - 1; end while (this_year != 1949)
%>

  <label for="diabetes_test_type">Diabetes Test Type</label>
  <% options=default.merge({
      :field_type => 'text',
      :id => "diabetes_test_type",
      :tt_pageStyleClass => 'LongSelectList'
    }) %>
  <%
  @diabetes_test_types = ['', 'CREATININE',
    'URINE PROTEIN',
    'VISUAL ACUITY',
    'FUNDOSCOPY',
    'FOOT CHECK',
    'UREA'
  ]
%>
<%#= select_tag( "diabetes_test_type", options_for_select(@diabetes_test_types), options) %>

  <%= hidden_field_tag( "diabetes_test_type",params[:diabetes_test_type],{:id=>"diabetes_test_type"}) %>

  <%= select_tag( "creatinine_unit", options_for_select(['', 'mg/dl', 'µmol/l']), {
      :helpText => 'Unit for Creatinine Value',
      :tt_requireNextClick => false,
      :condition => "$('diabetes_test_type').value.toLowerCase() == 'creatinine'"
    }) %>

  <%= text_field_tag "creatinine_result", nil,
    {:id => "diabetes_test_result",
    :helptext =>"Creatinine Result",
    :allowFreeText => 'true',
    :min => "0.5",
    :max => "3.6",
    :disabled => true,
    :condition => "$('diabetes_test_type').value.toLowerCase() == 'creatinine'",
    :field_type => 'number',
    :tt_onUnload => "updateCreatinineResult();",
    :tt_pageStyleClass => "Numeric NumbersOnlyWithDecimal",
    :tt_onload => "$('diabetes_type_concept').value = $('diabetes_test_type').value;
    $('diabetes_test_result').disabled = false;
    $('diabetes_type_concept').disabled = false;
    $('creatinine_result_value_numeric').disabled = false;
    $('infoBar'+tstCurrentPage).innerHTML = 'Creatinine (' + $('creatinine_unit').value + ')';
    resetCreatinineRange('touchscreenInput'+tstCurrentPage);
      " } %>
  <%= hidden_field_tag("observations[][value_numeric]", "", {:id => 'creatinine_result_value_numeric',:disabled => true}) %>
  <%= hidden_field_tag("observations[][concept_name]", "", {:id => 'diabetes_type_concept',:disabled => true}) %>
<%#= hidden_field_tag("observations[][patient_id]", @patient.id) %>
<%#= hidden_field_tag("observations[][obs_datetime]", '') %>

  <%= hidden_field_tag("urea_unit","mg/dl") %>
  <%= text_field_tag "observations[][value_numeric]", nil,
    {:id => "urea_result",
    :helptext =>"Urea Result",
    :allowFreeText => 'true',
    :disabled => true,
    :min => 0,
    :max => 25,
    :condition => "$('diabetes_test_type').value.toLowerCase() == 'urea'",
    :field_type => 'number',
    :tt_pageStyleClass => "Numeric NumbersOnlyWithDecimal",
    :tt_onload => "$('diabetes_type_concept').value = $('diabetes_test_type').value;
  $('urea_result').disabled = false;
  $('diabetes_type_concept').disabled = false;
  $('infoBar'+tstCurrentPage).innerHTML = 'Urea (' + $('urea_unit').value + ')';
    " } %>
